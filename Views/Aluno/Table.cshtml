<thead class="thead-dark">
    <tr>
        <th>Id</th>
        <th>Nome</th>
        <th>Turma</th>
        <th>Ações</th>
    </tr>
</thead>
<tbody>
    @foreach (var aluno in Model)
    {
        <tr>
            <td>@aluno.Id</td>
            <td>@aluno.Nome</td>
            <td>@aluno.TurmaNome</td>
            <td>
                <a href="/aluno/aluno?id=@aluno.Id" class="btn btn-outline-info">Mais</a>
                <a href="/aluno/Update?id=@aluno.Id" class="btn btn-outline-warning">Atualizar</a>
                <a href="/aluno/Delete?id=@aluno.Id" class="btn btn-outline-danger">Delete</a>
            </td>
        </tr>
    }
</tbody>